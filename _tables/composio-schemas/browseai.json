    {
      "slug": "BROWSEAI_CREATE_WEBHOOK",
      "name": "Create Webhook",
      "description": "This tool creates a new webhook for a BrowseAI robot. Webhooks are used to receive notifications when tasks are completed or data changes are detected. The webhook will be called with the task details when specific events occur. It is useful for: - Setting up automated notifications for task completion - Receiving real-time updates when changes are detected - Integrating BrowseAI with your own systems - Automating workflows based on robot task results The webhook can be configured to trigger on different events: - task.completed: When a task is successfully completed - task.failed: When a task fails - changes.detected: When changes are detected during monitoring",
      "inputParameters": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "description": "A description of the webhook",
            "title": "Description",
            "default": null,
            "nullable": true,
            "examples": [
              "Webhook for task completion notifications"
            ]
          },
          "events": {
            "type": "array",
            "description": "List of events to trigger the webhook",
            "title": "Events",
            "default": [
              "task.completed"
            ],
            "nullable": true,
            "items": {
              "type": "string",
              "properties": {}
            },
            "examples": [
              [
                "task.completed",
                "task.failed",
                "changes.detected"
              ]
            ]
          },
          "is_active": {
            "type": "boolean",
            "description": "Whether the webhook should be active immediately",
            "title": "Is Active",
            "default": true,
            "nullable": true,
            "examples": [
              true
            ]
          },
          "robot_id": {
            "type": "string",
            "description": "The ID of the robot to attach the webhook to",
            "title": "Robot Id",
            "examples": [
              "robot_abc123"
            ]
          },
          "url": {
            "type": "string",
            "description": "The URL where webhook notifications will be sent",
            "title": "Url",
            "minLength": 1,
            "format": "uri",
            "examples": [
              "https://example.com/webhook"
            ]
          }
        },
        "required": [
          "robot_id",
          "url"
        ],
        "title": "CreateWebhookRequest"
